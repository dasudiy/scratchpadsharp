{
  "Application": {
    "DeveloperMode": false,
    "RecentFiles": []
  },
  "Editor": {
    "FontFamily": "Cascadia Code",
    "FontSize": 14,
    "ShowLineNumbers": true,
    "TabSize": 4
  },
  "Execution": {
    "DefaultTimeoutSeconds": 30,
    "MaxMemoryMb": 512
  },
  "NuGet": {
    "DefaultSources": [
      "https://api.nuget.org/v3/index.json"
    ],
    "PackageCacheFolder": "./.packages"
  },
  "DefaultUsings": [
    "System",
    "System.Linq",
    "System.Collections.Generic",
    "Dumpify",
    "Microsoft.EntityFrameworkCore"
  ]
}
